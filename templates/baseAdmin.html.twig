<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<title>
			{% block title %}CMS
			{% endblock %}
		</title>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		{% block stylesheets %}
			<link href="{{ asset('assets/admin/css/styles.css')}}" rel="stylesheet"/>
			<link href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" crossorigin="anonymous"/>
			<link href="https://cdn.jsdelivr.net/gh/gitbrent/bootstrap4-toggle@3.6.1/css/bootstrap4-toggle.min.css" rel="stylesheet">

		{% endblock %}

	</head>
	<body></body>
	<body class="sb-nav-fixed">
		<nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
			<a class="navbar-brand" href="{{ path('admin') }}">CMS PORTFOLIO</a>

			<a class="navbar-brand float-right" href="{{ path('app_logout') }}">Zalogowany jako:
				{{ app.user.username }}</a>

		</nav>
		<div id="layoutSidenav">
			<div id="layoutSidenav_nav">
				<nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
					<div class="sb-sidenav-menu">
						<div class="nav">
							<div class="sb-sidenav-menu-heading">Core</div>
							<a class="nav-link" href="{{ path('admin') }}">
								<div class="sb-nav-link-icon">
									<i class="fas fa-tachometer-alt"></i>
								</div>
								Dashboard
							</a>

							<div class="sb-sidenav-menu-heading">Sekcje</div>
							<a class="nav-link {% if app.request.get('_route') starts with 'admin_about' %}active{% endif %}" href="{{ path('admin_about')}}">
								<div class="sb-nav-link-icon">
									<i class="fas fa-columns"></i>
								</div>
								O mnie
							</a>

							<a class="nav-link {% if app.request.get('_route') starts with 'admin_technologies' %}active{% endif %}" href="{{ path('admin_technologies')}}">
								<div class="sb-nav-link-icon">
									<i class="fas fa-table"></i>
								</div>
								Technologie
							</a>

							<a class="nav-link {% if app.request.get('_route') starts with 'admin_skills' %}active{% endif %}" href="{{ path('admin_skills')}}">
								<div class="sb-nav-link-icon">
									<i class="fas fa-table"></i>
								</div>
								Umiejętności
							</a>

							<a class="nav-link {% if app.request.get('_route') starts with 'admin_projects' %}active{% endif %}" href="{{ path('admin_projects')}}">
								<div class="sb-nav-link-icon">
									<i class="fas fa-table"></i>
								</div>
								Projekty
							</a>

							<a class="nav-link {% if app.request.get('_route') starts with 'admin_summary_numbers' %}active{% endif %}" href="{{ path('admin_summary_numbers')}}">
								<div class="sb-nav-link-icon">
									<i class="fas fa-table"></i>
								</div>
								Liczby podsumowujace
							</a>

							<a class="nav-link" href="#">
								<div class="sb-nav-link-icon">
									<i class="fas fa-envelope"></i>
								</div>
								Wiadomości
							</a>

						</div>
					</div>
				</nav>
			</div>
			<div id="layoutSidenav_content">
				<main>
					{% for message in app.flashes('success')%}
						<div class='alert alert-success'>
							{{ message }}
						</div>
					{% endfor %}

					{% for message in app.flashes('error')%}
						<div class='alert alert-danger'>
							{{ message }}
						</div>
					{% endfor %}
					<br>
					<div class="container-fluid">
						<div class="card mb-4"> {% block body %}{% endblock %}
							</div>
						</div>

					</main>
				</div>
			</body>
		</body>
	</html>
</div>
<footer class="py-4 bg-light mt-auto">
	<div class="container-fluid">
		<div class="d-flex align-items-center justify-content-between small float-right">
			<div class="text-muted">Copyright &copy; Your Website 2020</div>
		</div>
	</div>
</footer></body>{% block javascripts %}
<script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/js/all.min.js" crossorigin="anonymous"></script>
<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
<script src="{{ asset('assets/admin/js/scripts.js')}}"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.8.0/Chart.min.js" crossorigin="anonymous"></script>
<script src="{{ asset('assets/admin/assets/demo/chart-area-demo.js')}}"></script>
<script src="{{ asset('assets/admin/assets/demo/chart-bar-demo.js')}}"></script>
<script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js" crossorigin="anonymous"></script>
<script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js" crossorigin="anonymous"></script>
<script src="{{ asset('assets/admin/assets/demo/datatables-demo.js')}}"></script>
<script src="https://cdn.jsdelivr.net/gh/gitbrent/bootstrap4-toggle@3.6.1/js/bootstrap4-toggle.min.js"></script>
<script>
	$('#dataTable').DataTable({
"language": {
"sEmptyTable": "Brak danych w tabeli",
"sInfo": "Pokazuję _START_ do _END_ z _TOTAL_ wpisów",
"sInfoEmpty": "Pokazuję 0 do 0 z 0 wpisów",
"sInfoFiltered": "(Przefiltrowano z _MAX_ całkowitych wpisów)",
"sInfoPostFix": "",
"sInfoThousands": ",",
"sLengthMenu": "Pokaż _MENU_ wpisów",
"sLoadingRecords": "Ładuje...",
"sProcessing": "Przetwarzam...",
"sSearch": "Szukaj:",
"sZeroRecords": "Nie znaleziono pasującego wyniku",
"oPaginate": {
"sFirst": "Pierwszy",
"sLast": "Ostatni",
"sNext": "Następny",
"sPrevious": "Poprzedni"
},
"oAria": {
"sSortAscending": ": activate to sort column ascending",
"sSortDescending": ": activate to sort column descending"
}
}
});
$(function () {
$('[data-toggle="tooltip"]').tooltip()
})
</script>{% endblock %}</body></html>
